{
    "version": 2,
    "routes": [
        {
            "src": "/user/(.*)",
            "dest": "dist/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        },
        {
            "src": "/threads/(.*)",
            "dest": "dist/main.js",
            "methods": [
                "GET",
                "POST"
            ]
        },
        {
            "src": "/proposals/(.*)",
            "dest": "dist/main.js",
            "methods": [
                "GET",
                "POST"
            ]
        },
        {
            "src": "/events/(.*)",
            "dest": "dist/main.js",
            "methods": [
                "GET"
            ]
        }
    ]
}